<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.newkeepinshape">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.exercicio.ExercicioRegister"
            android:label="@string/title_activity_exercicio_register"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.exercicio.ExericioLista"
            android:label="@string/title_activity_exericio_lista"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>